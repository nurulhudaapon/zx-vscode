pub fn Page(allocator: zx.Allocator) zx.Component {
	var i: usize = 0;
	var j: usize = 3;

	return (
		<main @allocator={allocator}>
			<section>
				{while (i < 3) {
					const n = i;
					i += 1;
					(<p>{n}</p>)
				}}
			</section>

			<section>
				{while (j > 0) {
					j -= 1;
					(<p>{j}</p>)
				}}
			</section>
		</main>
	);
}

const zx = @import("zx");
